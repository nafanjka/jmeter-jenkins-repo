          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="false">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.bouncycastle.util.encoders.Base64
import com.ccpp.PKCS7
import org.apache.jmeter.samplers.SampleResult
import org.apache.commons.lang3.StringUtils

def response_modification = 2 // 0 - dirty, 1 - clean, 2 - summary

def code = prev.getResponseCode() as int

if (code in 400..599) {
    def msg = &quot;empty response because of Server error: $code&quot;
    prev.setResponseData(msg)
    prev.setDataType(SampleResult.TEXT)
    prev.setSuccessful(false)
    prev.setResponseMessage(&quot;Assertion failed: $msg&quot;)
} else if (code in 200..299) {
    def privateKey = props.get(&quot;private_key&quot;)
    def dirty = prev.getResponseDataAsString()
    def clean = PKCS7.decrypt(privateKey, &quot;2c2p&quot;, Base64.decode(dirty), &quot;pwdpwdpwd&quot;)
    def respCodeTag = StringUtils.substringBetween(clean, &quot;&lt;respCode&gt;&quot;, &quot;&lt;/respCode&gt;&quot;)
    def invoiceNo = StringUtils.substringBetween(clean, &quot;&lt;uniqueTransactionCode&gt;&quot;, &quot;&lt;/uniqueTransactionCode&gt;&quot;)
    def status = StringUtils.substringBetween(clean, &quot;&lt;status&gt;&quot;, &quot;&lt;/status&gt;&quot;)
    def reason = StringUtils.substringBetween(clean, &quot;&lt;failReason&gt;&quot;, &quot;&lt;/failReason&gt;&quot;)
    def summary = &quot;Status server: $code || respCode: $respCodeTag || invoiceNo: $invoiceNo || status: $status || reason: $reason&quot;
    if (respCodeTag == &quot;00&quot;) {
        if (response_modification == 0) {
            prev.setResponseData(dirty)
        } else if (response_modification == 1) {
            prev.setResponseData(clean)
        } else if (response_modification == 2) {
            prev.setResponseData(summary)
        }
        prev.setSuccessful(true)
    } else {
        prev.setResponseData(summary)
        prev.setSuccessful(false)
        prev.setDataType(SampleResult.TEXT)
        prev.setResponseMessage(&quot;Assertion failed: $summary&quot;)
    }
}

// Loop control
def threadNum = ctx.getThreadNum()
def maxLoops = (props.get(&quot;loop_count&quot;) ?: &quot;0&quot;) as int
if (maxLoops &gt; 0) {
    def key = &quot;executedLoops_${threadNum}&quot;
    def executed = (vars.get(key) ?: &quot;0&quot;) as int
    executed++
    vars.put(key, executed.toString())
    if (executed &gt;= maxLoops) ctx.getThreadGroup().stopThread(threadNum, true)
}</stringProp>
