          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="inputs" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">if (prev.getResponseCode() == &quot;200&quot;) {
    def jobConfig = vars.getObject(&quot;job_config&quot;)
    if (jobConfig &amp;&amp; jobConfig.size() != 0) {
        def keys = [
            &quot;server&quot;, &quot;num_of_threads&quot;, &quot;ramp_up&quot;, &quot;loop_count&quot;, &quot;test_duration&quot;,
            &quot;merchant_id&quot;, &quot;merchant_secretkey&quot;, &quot;invoice_prefix&quot;, &quot;pan&quot;, &quot;expiry_month&quot;,
            &quot;expiry_year&quot;, &quot;cvv&quot;, &quot;description&quot;, &quot;currency_code&quot;, &quot;payment_amount&quot;,
            &quot;pub_cert&quot;, &quot;private_key&quot;, &quot;email&quot;, &quot;multicurrency&quot;, &quot;version&quot;, &quot;localORstoredCard&quot;
        ]
        def config = jobConfig.get(0)
        keys.each { key -&gt;
            def value = config.get(key)
            props.put(key, value != null ? value.toString() : &quot;&quot;)
            log.info(&quot; $key: $value&quot;)
        }
    } else {
        FailureMessage = &quot;!!!!!!!!!!!! The response is empty !!!!!!!!!!!!!&quot;
        Failure = true
        prev.setStopThread(true)
    }
} else {
    FailureMessage = &quot;!!!!!!!!!!!! No connection to the database !!!!!!!!!!!!!&quot;
    Failure = true
    prev.setStopThread(true)
}

def sleepTime = 5 // seconds
def step = 1 // seconds

log.info(&quot;Preparing for Main load test run ...&quot;)
for (int t = sleepTime; t &gt; 0; t -= step) {
    log.info(&quot;Readiness: &quot; + t + &quot; s left&quot;)
    Thread.sleep(step * 1000) // sleep for 1 second
}
log.info(&quot;Main Load test execution...&quot;)
Thread.sleep(2000)</stringProp>
          </JSR223Assertion>
